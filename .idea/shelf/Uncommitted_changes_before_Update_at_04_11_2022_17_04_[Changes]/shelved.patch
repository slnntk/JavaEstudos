+import java.util.Scanner;
+
+public class Exercicios {
+
+    public void AtribuicaoAcumulativa() {
+
+        double valor = 50;
+
+        Scanner sc = new Scanner(System.in);
+        double minutos = sc.nextDouble();
+
+        if (minutos <= 100) {
+            System.out.printf("Valor a pagar: R$: %.2f", valor);
+        } else {
+            valor += (minutos - 100) * 2;
+            System.out.printf("Valor a pagar: R$: %.2f", valor);
+        }
+    }
+
+    public void SwitchCase() {
+
+        Scanner sc = new Scanner(System.in);
+        int dia = sc.nextInt();
+        String nome;
+
+        switch (dia) {
+            case 1:
+                nome = "domingo";
+                break;
+            case 2:
+                nome = "segunda";
+                break;
+            case 3:
+                nome = "terça";
+                break;
+            case 4:
+                nome = "quarta";
+                break;
+            case 5:
+                nome = "quinta";
+                break;
+            case 6:
+                nome = "sexta";
+                break;
+            case 7:
+                nome = "sabado";
+                break;
+            default:
+                nome = "valor invalido";
+                break;
+        }
+        System.out.println("Dia da semana: " + nome);
+    }
+
+    public void CondicionalTernaria(){
+        double preco = 34.5;
+        double desconto = (preco < 20.0) ? preco * 0.1 : preco * 0.05;
+        System.out.println(desconto);
+    }
+
+    public void ExemploAula40(){
+        double price = 400.00;
+        double desconto = 0;
+        desconto = (price < 200) ? price * 0.10 : price * 0.20;
+        System.out.print(desconto);
+    }
+
+
+
+
+}
